
# -------- Désactive les magic quotes -------- #
SetEnv MAGIC_QUOTES 0


# -------- Active PHP 5 -------- #
SetEnv PHP_VER 5


IndexIgnore */*

RewriteEngine on





# ------------------------ #
# -------- ASSETS -------- #
# ------------------------ #


# -------- À DÉSACTIVER EN PROD -------- #
RewriteRule json/(.*)$ assets/json/$1 [L]
RewriteRule img/(.*)$ assets/img/$1 [L]
